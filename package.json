  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test"
  },
  "dependencies": {
    "ember-cli-babel": "^6.7.2",
    "ember-sinon": "~1.0.0"
  },
  "devDependencies": {
    "ember-cli": "2.15.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.0",
