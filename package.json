{
  "name": "ember-sinon-qunit",
  "version": "6.0.0",
  "description": "Sinon sandbox test integration for QUnit",
  "keywords": [
    "ember-addon",
    "sinon",
    "qunit"
  ],
  "bugs": {
    "url": "https://github.com/elwayman02/ember-sinon-qunit/issues"
  },
  "repository": "https://github.com/elwayman02/ember-sinon-qunit",
  "license": "MIT",
  "contributors": [
    "Jordan Hawker <hawker.jordan@gmail.com> (https://www.JordanHawker.com)",
    "Steve Calvert <steve.calvert@gmail.com>"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.8",
    "ember-auto-import": "^2.5.0",
    "ember-cli-babel": "^7.26.11",
    "sinon": "^15.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.0.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@release-it-plugins/lerna-changelog": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.6.0",
    "ember-cli": "~4.9.2",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^4.9.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^9.0.1",
    "ember-source": "~4.9.3",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.3.1",
    "ember-try": "^2.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ember": "^11.4.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.2",
    "qunit": "^2.19.3",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.6.0",
    "util": "^0.12.5",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "volta": {
    "node": "14.21.1",
    "yarn": "1.22.19"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-sinon"
    ]
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
