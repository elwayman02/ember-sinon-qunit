  "description": "Sinon sandbox test integration for QUnit",
  "keywords": [
    "ember-addon",
    "sinon",
    "qunit"
  ],
  "license": "MIT",
  "author": "Jordan Hawker <hawker.jordan@gmail.com> (http://www.JordanHawker.com)",
