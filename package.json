{
  "name": "ember-sinon-qunit",
  "version": "5.0.0",
  "description": "Sinon sandbox test integration for QUnit",
  "keywords": [
    "ember-addon",
    "sinon",
    "qunit"
  ],
  "repository": "https://github.com/elwayman02/ember-sinon-qunit",
  "license": "MIT",
  "contributors": [
    "Jordan Hawker <hawker.jordan@gmail.com> (http://www.JordanHawker.com)",
    "Steve Calvert <steve.calvert@gmail.com>"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/elwayman02/ember-sinon-qunit/issues"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.3",
    "ember-cli-babel": "^7.23.0",
    "ember-sinon": "^5.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "babel-eslint": "^10.0.3",
    "ember-auto-import": "^1.11.2",
    "ember-cli": "~3.23.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^2.0.2",
    "ember-cli-uglify": "^3.0.0",
    "ember-data": "^3.23.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.24.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "^7.7.2",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^1.6.0",
    "sinon": "^9.2.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-sinon"
    ]
  }
}
